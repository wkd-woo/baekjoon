   "source": [
    "from itertools import permutations\n",
    "n = int(input())\n",
    "l = list(map(int, input().split()))\n",
    "mx = -int(1e9)\n",
    "\n",
    "for one in permutations(l, n):\n",
    "    temp = 0\n",
    "    for i in range(0, n - 1):\n",
    "        temp += abs(one[i] - one[i + 1])\n",
    "    mx = max(mx, temp)\n",
    "\n",
    "print(mx)"
   ],
