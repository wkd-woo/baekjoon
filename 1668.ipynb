   "source": [
    "def ascending(l):\n",
    "    now = l[0]\n",
    "    result = 1\n",
    "    for i in range(1, len(l)):\n",
    "        if now < l[i]:\n",
    "            result += 1\n",
    "            now = l[i]\n",
    "    return result\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "l = [int(input()) for _ in range(n)]\n",
    "print(ascending(l))\n",
    "l.reverse()\n",
    "print(ascending(l))"
